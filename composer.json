{
    "name": "nandaniya480/blog",
    "description": "Simple Blog Package",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Govind Nandaniya"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2@dev",
        "orchestra/testbench": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Nandaniya480\\Blog\\": "src/",
            "Nandaniya480\\Blog\\Tests\\": "src/tests/"

        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nandaniya480\\Blog\\Providers\\BlogProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
